<Page
    x:Class="CnBetaUWA.BlankPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:controls="using:CnBetaUWA.Controls"
    NavigationCacheMode="Disabled"
    mc:Ignorable="d">
    <Border Background="LightBlue">
        <controls:ExtendSplitView DisplayMode="CompactOverlay">
            <SplitView.Pane>
                <Button Content="Text"/>
            </SplitView.Pane>
            <SplitView.Content>
                <Frame x:Name="VMFrame1" SourcePageType="{Binding SelectedPageType, Mode=TwoWay}"/>
            </SplitView.Content>
        </controls:ExtendSplitView>
    </Border>
    </Page>
    