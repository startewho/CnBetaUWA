<Page
    x:Class="CnBetaUWA.BlankPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:controls="using:CnBetaUWA.Controls"
    NavigationCacheMode="Disabled"
    mc:Ignorable="d">
    <Grid Background="LightBlue">
        <controls:LabelSlider   Minimum="1"  Maximum="5" TickFrequency="1"  TickPlacement="Inline" IsThumbToolTipEnabled="False" ThumbToolTipValueConverter="{StaticResource SliderValueConverter}">
            <controls:LabelSlider.TickTemplate>
                <DataTemplate>
                    <Ellipse Fill="Blue" Width="3" Height="3" />
                </DataTemplate>
            </controls:LabelSlider.TickTemplate>
        </controls:LabelSlider>
    </Grid>
    </Page>
    